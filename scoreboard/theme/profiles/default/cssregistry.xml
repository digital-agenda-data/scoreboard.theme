<?xml version="1.0"?>
<object name="portal_css">

 <!-- Set conditions on stylesheets we don't want to pull in -->
 <stylesheet
  expression="python: (not request.get('HTTP_X_THEME_ENABLED', None)) or '@@theming-controlpanel' in request.getURL()"
  id="public.css"
  />
<stylesheet
  id="++resource++scoreboard.theme.css/listing.css"
  />
<stylesheet
  id="++resource++scoreboard.theme.css/bootstrap.css"
  />
<stylesheet
  id="ie8.css"
  conditionalcomment="IE 8"
  />
<stylesheet
  id="ie9.css"
  conditionalcomment="IE 9"
  />
</object>

